{
  "name": "FMTCco/php-integrations",
  "description": "A simple reusable PHP wrapper for handling affiliate networks",
  "keywords": [
    "php",
    "networks",
    "api",
    "FMTC"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/FMTCco/php-integrations",
  "authors":[
    {
      "name": "James Weston",
      "email": "james@savannah.io"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^6.2",
    "jamesvweston/php-utilities": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "FMTCco\\Integrations\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
